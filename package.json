  "devDependencies": {
    "@types/bun": "^1.1.8",
    "@types/node": "^20.10.5",
    "astro": "^4.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
